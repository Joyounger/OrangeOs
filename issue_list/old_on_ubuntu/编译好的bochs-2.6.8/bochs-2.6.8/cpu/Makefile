BX_INCLUDES = ../bochs.h ../config.h


all: libcpu.a

.cc.o:
	$(CXX) -c $(BX_INCDIRS) $(CXXFLAGS) $< -o $@


libcpu.a: $(OBJS) 
	rm -f  libcpu.a
	ar rv $@ $(OBJS) 
	$(RANLIB) libcpu.a

$(OBJS): $(BX_INCLUDES)

$(OBJS64): $(BX_INCLUDES)

clean:
	rm -f  *.o
	rm -f  *.a

dist-clean: clean
	rm -f  Makefile

